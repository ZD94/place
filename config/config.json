{
  "appName": "${JL_APP_NAME}",
  "listen": "${JL_APP_PORT}",
  "cluster": true,
  "postgres": {
    "url": "${JL_APP_POSTGRES_URL}"
  },
  "dataDir": "${rootDir}/data",
  "cron": "0 0 1 * * *",
  "rootDir": "${JL_APP_DATA_DIR}",
  "logger": {
    "path": "${{rootDir}}/log",
    "prefix": "${{appName}}_",
    "httptype": "short",
    "mods": {
      "sequelize": { "prefix": "${{appName}}_sql_", "console": false },
      "sqlinit": { "prefix": "${{appName}}_sql_", "console": false },
      "http": {
        "mods": {
          "access": { "console": false }
        }
      }
    }
  }
}

